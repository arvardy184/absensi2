{
  "expo": {
    "name": "AbsensiKelas",
    "slug": "absensi-kelas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.absensi.kelas",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png",
      "name": "Sistem Absensi Kelas",
      "shortName": "Absensi",
      "lang": "id",
      "scope": "/",
      "themeColor": "#2196F3",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "crossorigin": "use-credentials"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
